{
  "extends": "airbnb",
  "installedESLint": true,
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "meteor": true
  },
  "globals": {
    "Messages": true,
    "HMISAPI": true,
    "clientInfo": true,
    "Sortable": true,
    "OAuth": true,
    "logger": true,
    "moment": true,
    "AdminConfig": true,
    "adminCollectionObject": true,
    "AccountsTemplates": true,
    "Roles": true,
    "RouteController": true,
    "HomeAppController": true,
    "HomeAppSurveyorController": true,
    "responses": true,
    "users": true,
    "surveyQuestionsMaster": true,
    "questions": true,
    "surveys": true,
    "options": true,
    "rolePermissions": true,
    "homeRoles": true,
    "Schemas": true,
    "HomeHelpers": true,
    "AdminDashboard": true,
    "AutoForm": true,
    "SimpleSchema": true,
    "Gravatar": true,
    "messages": true,
    "AdminTables": true,
    "MeteorCamera": true,
    "Geolocation": true,
    "GoogleMaps": true,
    "google": true,
    "LocationTracker": true,
    "housingInventory": true
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id",
          "_redirectUri"
        ]
      }
    ]
  }
}
