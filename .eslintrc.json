{
  "extends": "airbnb",
  "installedESLint": true,
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "meteor": true
  },
  "globals": {
    "Messages": true,
    "HMISAPI": true,
    "clientInfo": true,
    "Sortable": true,
    "OAuth": true,
    "logger": true,
    "moment": true,
    "AdminConfig": true
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id",
          "_redirectUri"
        ]
      }
    ]
  }
}
